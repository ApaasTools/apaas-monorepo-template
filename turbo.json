{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "zip/**"]
    },
    "build:*": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:*": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "outputs": [".turbo", "dist", "build", "zip"]
    },
    "build:wc": {
      "cache": false,
      "persistent": true
    }
  }
}
